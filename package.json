{
  "name": "easy-factory",
  "version": "1.0.2",
  "description": "Simple base class for a consistent factory pattern.",
  "main": "lib/factory.js",
  "scripts": {
    "test": "istanbul cover --print both nodeunit ./test",
    "lint": "eslint --quiet . --",
    "coverage": "istanbul check-coverage --statements 95 --lines 95 --branches 95 --functions 95",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "author": "Mateu Aguiló Bosch <mateu.aguilo.bosch@gmail.com>",
  "contributors": [
    "Mateu Aguiló Bosch <mateu.aguilo.bosch@gmail.com>"
  ],
  "license": "GPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e0ipso/easy-factory.git"
  },
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^4.3.0",
    "ghooks": "^2.0.0",
    "istanbul": "^0.4.2",
    "nodeunit": "^0.11.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "bugs": {
    "url": "https://github.com/e0ipso/easy-factory/issues"
  },
  "homepage": "https://github.com/e0ipso/easy-factory#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "design",
    "patterns",
    "factory"
  ]
