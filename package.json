    }
  ],
  "repository": "e0ipso/simple-factory",
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^1.10.3",
    "ghooks": "^1.0.1",
    "istanbul": "^0.4.2",
    "nodeunit": "^0.9.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
